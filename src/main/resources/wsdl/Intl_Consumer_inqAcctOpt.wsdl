         <!-- inqAcctOpt -->
         <!--******  The inqAcctOpt Packet ******-->
         <!--*****  Packet Request Element ******-->
         <xsd:element name="inqAcctOpt" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:annotation id="inqAcctOpt">
               <xsd:documentation>You can use the inqAcctOpt operation to receive information about option sets assigned to an account. This operation returns codes for the option sets and the description for each option set. You can request specific option sets or all option sets assigned to the account. If the option set is returned with zeroes or the option set element is not returned at all, no option set is assigned.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element ref="tsys:inqAcctOptRequest" />
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <!--*****  Packet Response Element ******-->
         <xsd:element name="inqAcctOptResponse" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:complexType>
