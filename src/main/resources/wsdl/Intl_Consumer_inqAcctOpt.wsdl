               <xsd:element name="cardReceiptVerif" minOccurs="0" nillable="true">
                  <xsd:annotation id="inqAcctOpt.inqAcctOptResponse.acctOptData.cardReceiptVerif">
                     <xsd:documentation>Code for the card receipt verification (CRV) option set assigned to the account.&lt;br&gt;This option set is used to determine which cards must be verified by the customer and whether dual accounts and all customers are automatically activated.</xsd:documentation>
