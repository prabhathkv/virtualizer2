                     <xsd:documentation>Description of the collection option set.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1" />
                        <xsd:maxLength value="25" />
                     </xsd:restriction>
