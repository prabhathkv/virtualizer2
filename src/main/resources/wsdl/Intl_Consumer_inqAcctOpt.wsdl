               </xsd:element>
               <xsd:element name="auth" minOccurs="0" nillable="true">
                  <xsd:annotation id="inqAcctOpt.inqAcctOptResponse.acctOptData.auth">
                     <xsd:documentation>Code for the authorization option set assigned to the account.&lt;br&gt;This option set is used to define how the authorization pad is calculated and when authorizations are purged.</xsd:documentation>
