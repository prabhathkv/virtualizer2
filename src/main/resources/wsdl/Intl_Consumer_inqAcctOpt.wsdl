               </xsd:element>
               <xsd:element name="security" minOccurs="0" nillable="true">
                  <xsd:annotation id="inqAcctOpt.inqAcctOptResponse.acctOptData.security">
                     <xsd:documentation>Code for the security fraud option set assigned to the account.&lt;br&gt;This option set is used to determine how card security reports and transfers are processed and how exceptions and accounts qualify for queueing.</xsd:documentation>
